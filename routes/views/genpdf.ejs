<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Enquiry pdf</title>
    <link rel="shortcut icon" href="/logo.jpeg" />

    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,400;0,500;0,700;0,900;1,400;1,500;1,700&amp;display=swap"
      rel="stylesheet"
    />

    <link
      rel="stylesheet"
      href="../../../client/public/assets/plugins/bootstrap/css/bootstrap.min.css"
    />

    <link
      rel="stylesheet"
      href="../../../client/public/assets/plugins/feather/feather.css"
    />

    <link
      rel="stylesheet"
      href="../../../client/public/assets/plugins/icons/flags/flags.css"
    />

    <link
      rel="stylesheet"
      href="../../../client/public/assets/plugins/fontawesome/css/fontawesome.min.css"
    />
    <link
      rel="stylesheet"
      href="../../../client/public/assets/plugins/fontawesome/css/all.min.css"
    />

    <link rel="stylesheet" href="../../../client/public/assets/css/style.css" />
    <style>
      body {
        width: 100%;
        height: 2000px;
        margin: 0;
        padding: 0;
      }
    </style>
  </head>
  <body>
    <div
      className="main-wrapper border"
      style="
        width: 185mm;
        height: 270mm;
        margin: 0;
        padding: 3mm;
        background-color: white;
        border: 1px solid black;
      "
    >
      <div className="page-wrapper">
        <div className="content container-fluid">
          <div className="row justify-content-center">
            <div className="col-xl-6 mx-4">
              <div className="card invoice-info-card">
                <div className="card-body">
                  <div className="invoice-item invoice-item-one">
                    <div className="row">
                      <div className="col-md-12">
                        <div className="invoice-logo">
                          <img
                            src="http://management12.netlify.app/logo.jpeg"
                            width="120"
                            height="120"
                            alt="Image"
                            style="vertical-align: middle"
                          />
                          <h2
                            style="
                              display: inline-block;
                              color: rgb(32, 91, 229);
                            "
                          >
                            AEGIS PROJECTS TECHNOLOGY PVT. LTD
                          </h2>
                          <p style="display: inline-block">
                            Date : - <%= new Date().toLocaleString() %>
                          </p>
                        </div>
                      </div>
                    </div>
                  </div>
                  <br />
                  <hr />
                  <br />

                  <div className="invoice-item invoice-table-wrap">
                    <div className="row">
                      <div className="col-md-12">
                        <div className="table-responsive">
                          <strong className="customer-text-one">
                            A. MATERIAL DESCRIPTION :
                          </strong>
                          <br /> <br>
                          <table
                            className="invoice-table table table-center my-3"
                          >
                            <thead>
                              <tr>
                                <th>Make</th>
                                <th>Modal No</th>
                                <th>Part Name</th>
                                <th>Part No</th>
                              </tr>
                            </thead>
                            <% if(itemList.length >0){ %> <%
                            itemList.forEach(item=>{%>
                            <tr key="1">
                              <td><%= item.item_make %></td>
                              <td><%= item.item_modalNo %></td>
                              <td><%= item.item_name %></td>
                              <td><%= item.item_partNo %></td>
                            </tr>
                            <% }) %> <%} %>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
